<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="fiveStar">
        <attr name="starDistance" format="dimension"/>
        <attr name="starSize" format="dimension"/>
        <attr name="starCount" format="integer"/>
        <attr name="emptyDraw" format="reference"/>
        <attr name="fillDraw" format="reference"/>
    </declare-styleable>

    <declare-styleable name="waveView">
        <attr name="oneColor" format="color"/>
        <attr name="twoColor" format="color"/>
    </declare-styleable>

    <declare-styleable name="waweCircle">
        <attr name="circleColor" format="reference|color"/>
        <attr name="waveColor" format="reference|color"/>
        <attr name="textColor" format="reference|color"/>
        <attr name="textSize" format="reference|dimension"/>
    </declare-styleable>
</resources>